<h1>Listing tries</h1>

<table>
  <tr>
    <th>Position</th>
    <th>Amount</th>
    <th>Multiplicator</th>
    <th>Total</th>
    <th>Player</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tries.each do |try| %>
  <tr>
    <td><%= try.position %></td>
    <td><%= try.amount %></td>
    <td><%= try.multiplicator %></td>
    <td><%= try.total %></td>
    <td><%= try.player_id %></td>
    <td><%= link_to 'Show', try %></td>
    <td><%= link_to 'Edit', edit_try_path(try) %></td>
    <td><%= link_to 'Destroy', try, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Try', new_try_path %>
